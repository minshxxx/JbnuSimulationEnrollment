             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProjectA.View"
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="407">

    <Grid Width="407" Height="60">
